# sudo password

ansible_sudo_pass:

# .gitconfig vars

email:

full_name:

# Gnome theme name

gnome_theme: Adapta-Eta

# GitHub API token with gist read/write permissions and VSCode settings created by
# Settings Sync extension, read more: https://marketplace.visualstudio.com/items?itemName=Shan.code-settings-sync

github_api_token:
vscode_gist_id:

# pyenv

python3_version: 3.8.0
python2_version: 2.7.16
pyenv_globals: 3.8.0 2.7.16

# nvm

node_version: 8.11.4
nvm_version: 0.34.0

# tmux

tmux_version: '2.9'

# termite

termite_config_file: 'config.nord'

# zsh

zsh_version: '5.5'

# psql

psql_role_perms: SUPERUSER CREATEDB CREATEROLE INHERIT LOGIN  # be careful with these
psql_role_pass:  # your user's role password
psql_postgres_pass:  # postgres' role password

# redis

redis_pass:

# elasticsearch

elasticsearch_version: '6.2.4'

# ag

ag_version: '2.1.0'

# gron

gron_version: '0.5.2'

# resty

resty_version: 'master'

# slack

slack_version: '4.2.0'

# tmuxinator

project_name:

# docker

docker_compose_version: '1.25.0'

# flameshot

override_default_keybindings: true
